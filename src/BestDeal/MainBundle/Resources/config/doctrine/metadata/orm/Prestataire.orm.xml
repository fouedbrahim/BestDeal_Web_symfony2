<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Prestataire" table="prestataire">
    <id name="idPrestataire" type="integer" column="id_prestataire">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="45" nullable="false"/>
    <field name="prenom" type="string" column="prenom" length="45" nullable="false"/>
    <field name="email" type="string" column="email" length="45" nullable="false"/>
    <field name="tel" type="string" column="tel" length="45" nullable="false"/>
    <field name="adresse" type="string" column="adresse" length="45" nullable="false"/>
    <field name="pseudo" type="string" column="pseudo" length="45" nullable="false"/>
    <field name="mdp" type="string" column="mdp" length="45" nullable="false"/>
    <field name="statut" type="boolean" column="statut" nullable="false"/>
  </entity>
</doctrine-mapping>
